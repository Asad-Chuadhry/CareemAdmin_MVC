
@{
    ViewBag.Title = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="overview-wrap">
                        <h2 class="title-1">Customers</h2>
                    </div>
                    <br />
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <!-- DATA TABLE -->
                    <div class="table-responsive table--no-card m-b-30">
                        <table class="table table-borderless table-striped table-earning">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Phone Number</th>
                                    <th>Rides</th>
                                    <th>City</th>
                                    <th>Payments</th>
                                    <th>Join Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var c in ViewBag.searchList)
                                {
                                    <tr>
                                        <td>@c.name</td>
                                        <td>@c.phoneNo</td>
                                        <td>@c.rides</td>
                                        <td>@c.city</td>
                                        <td>@c.payment</td>
                                        <td>@c.date</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- END DATA TABLE -->
                </div>
            </div>
        </div>
    </div>
